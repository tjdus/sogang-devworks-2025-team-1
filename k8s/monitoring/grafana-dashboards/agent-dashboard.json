{
  "annotations": {
    "list": []
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "title": "Request Rate (by service)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "sum(rate(agent_requests_total[5m])) by (service, status)",
          "legendFormat": "{{service}} - {{status}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        }
      }
    },
    {
      "title": "Evaluation Score Distribution",
      "type": "histogram",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "sum(rate(agent_evaluation_score_bucket[5m])) by (le)",
          "legendFormat": "{{le}}"
        }
      ]
    },
    {
      "title": "Average Evaluation Score",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "sum(rate(agent_evaluation_score_sum[5m])) / sum(rate(agent_evaluation_score_count[5m]))",
          "legendFormat": "avg score"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "max": 1,
          "thresholds": {
            "steps": [
              { "color": "red", "value": 0 },
              { "color": "yellow", "value": 0.5 },
              { "color": "green", "value": 0.7 }
            ]
          }
        }
      }
    },
    {
      "title": "Response Latency (P50 / P95 / P99)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(agent_request_duration_seconds_bucket[5m])) by (le, service))",
          "legendFormat": "P50 - {{service}}"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(agent_request_duration_seconds_bucket[5m])) by (le, service))",
          "legendFormat": "P95 - {{service}}"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(agent_request_duration_seconds_bucket[5m])) by (le, service))",
          "legendFormat": "P99 - {{service}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    },
    {
      "title": "Active Prompt Version",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 16 },
      "targets": [
        {
          "expr": "agent_prompt_version",
          "legendFormat": "{{task_type}}"
        }
      ]
    },
    {
      "title": "Evaluation Pass/Fail Rate",
      "type": "piechart",
      "gridPos": { "h": 8, "w": 6, "x": 6, "y": 16 },
      "targets": [
        {
          "expr": "sum(agent_evaluation_pass_total) by (result)",
          "legendFormat": "{{result}}"
        }
      ]
    },
    {
      "title": "Optimization Runs",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 },
      "targets": [
        {
          "expr": "sum(increase(agent_optimization_runs_total[10m])) by (task_type, result)",
          "legendFormat": "{{task_type}} - {{result}}"
        }
      ]
    },
    {
      "title": "Error Rate (%)",
      "type": "gauge",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 20 },
      "targets": [
        {
          "expr": "sum(rate(agent_requests_total{status=\"error\"}[5m])) / sum(rate(agent_requests_total[5m])) * 100"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "steps": [
              { "color": "green", "value": 0 },
              { "color": "yellow", "value": 10 },
              { "color": "red", "value": 30 }
            ]
          }
        }
      }
    }
  ],
  "schemaVersion": 39,
  "tags": ["agent-system"],
  "templating": { "list": [] },
  "time": { "from": "now-1h", "to": "now" },
  "title": "Agent System Dashboard",
  "uid": "agent-system-dashboard"
}
