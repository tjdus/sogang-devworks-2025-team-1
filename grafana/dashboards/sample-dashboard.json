{
  "id": null,
  "uid": "agent-system-dashboard",
  "title": "Agent System Monitoring",
  "tags": ["agent", "system", "monitoring"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Total DB Logs",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "app_db_log_total",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"]
        }
      }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Recent Failures (24h)",
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 0 },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "app_db_recent_failures",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"]
        }
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "value": 0, "color": "green" },
              { "value": 3, "color": "yellow" },
              { "value": 10, "color": "red" }
            ]
          }
        }
      }
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Service Health (up status)",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "up{job=~\"manager|worker|evaluator\"}",
          "legendFormat": "{{job}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "stepAfter"
          }
        }
      }
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "HTTP Request Rate (req/s)",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "rate(http_requests_total[5m])",
          "legendFormat": "{{job}} - {{method}} {{path}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "HTTP Request Duration (95th percentile)",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
          "legendFormat": "{{job}} - {{method}} {{path}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "DB Log Growth Over Time",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "app_db_log_total",
          "legendFormat": "Total Logs",
          "refId": "A"
        },
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "app_db_recent_failures",
          "legendFormat": "Recent Failures",
          "refId": "B"
        }
      ]
    },
    {
      "id": 7,
      "type": "table",
      "title": "Recent Execution Logs (Last 50)",
      "gridPos": { "h": 12, "w": 24, "x": 0, "y": 20 },
      "targets": [
        {
          "datasource": { "type": "postgres", "uid": "postgresql" },
          "rawSql": "SELECT\n  id,\n  request_id,\n  task_type,\n  prompt_version,\n  evaluation_passed,\n  evaluation_score,\n  worker_latency_ms,\n  created_at,\n  user_input,\n  refined_input,\n  worker_output,\n  error_message\nFROM execution_logs\nORDER BY created_at DESC\nLIMIT 50;\n",
          "refId": "A",
          "format": "table"
        }
      ],
      "options": {
        "showHeader": true,
        "sortBy": [
          {
            "displayName": "created_at",
            "desc": true
          }
        ]
      },
      "fieldConfig": {
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "evaluation_passed" },
            "properties": [
              {
                "id": "custom.displayMode",
                "value": "color-background"
              },
              {
                "id": "mappings",
                "value": [
                  {
                    "type": "value",
                    "options": {
                      "true": { "color": "green", "text": "passed", "index": 0 },
                      "false": { "color": "red", "text": "failed", "index": 1 }
                    }
                  }
                ]
              }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "evaluation_score" },
            "properties": [
              {
                "id": "custom.displayMode",
                "value": "gradient-gauge"
              },
              {
                "id": "max",
                "value": 1
              },
              {
                "id": "min",
                "value": 0
              }
            ]
          }
        ]
      }
    },
    {
      "id": 8,
      "type": "table",
      "title": "Failed Executions Details",
      "gridPos": { "h": 12, "w": 24, "x": 0, "y": 32 },
      "targets": [
        {
          "datasource": { "type": "postgres", "uid": "postgresql" },
          "rawSql": "SELECT\n  id,\n  request_id,\n  task_type,\n  prompt_version,\n  evaluation_score,\n  evaluation_passed,\n  worker_latency_ms,\n  created_at,\n  user_input,\n  refined_input,\n  worker_output,\n  error_message\nFROM execution_logs\nWHERE error_message IS NOT NULL\n   OR evaluation_passed = false\nORDER BY created_at DESC\nLIMIT 30;\n",
          "refId": "A",
          "format": "table"
        }
      ],
      "options": {
        "showHeader": true
      }
    },
    {
      "id": 9,
      "type": "timeseries",
      "title": "Success Rate by Task Type",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 44 },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "sum(rate(http_requests_total{status=\"200\"}[5m])) by (job) / sum(rate(http_requests_total[5m])) by (job)",
          "legendFormat": "{{job}} success rate",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1
        }
      }
    },
    {
      "id": 10,
      "type": "stat",
      "title": "Average Score (Last Hour)",
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 44 },
      "targets": [
        {
          "datasource": { "type": "postgres", "uid": "postgresql" },
          "rawSql": "SELECT\n  AVG(evaluation_score) AS avg_score\nFROM execution_logs\nWHERE created_at >= NOW() - INTERVAL '1 hour'\n  AND evaluation_score IS NOT NULL;",
          "refId": "A",
          "format": "table"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"]
        }
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "value": 0, "color": "red" },
              { "value": 0.5, "color": "yellow" },
              { "value": 0.7, "color": "green" }
            ]
          },
          "decimals": 3
        }
      }
    },
    {
      "id": 11,
      "type": "stat",
      "title": "Pass Rate (Last Hour)",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 44 },
      "targets": [
        {
          "datasource": { "type": "postgres", "uid": "postgresql" },
          "rawSql": "SELECT\n  AVG(CASE WHEN evaluation_passed = true THEN 1.0 ELSE 0.0 END) AS pass_rate\nFROM execution_logs\nWHERE created_at >= NOW() - INTERVAL '1 hour'\n  AND evaluation_passed IS NOT NULL;",
          "refId": "A",
          "format": "table"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"]
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "thresholds": {
            "steps": [
              { "value": 0, "color": "red" },
              { "value": 0.7, "color": "yellow" },
              { "value": 0.9, "color": "green" }
            ]
          },
          "decimals": 1
        }
      }
    }
  ]
}
