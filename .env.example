# Database
DATABASE_URL=postgresql+asyncpg://agent:agent1234@localhost:5432/agent_system

# LLM Provider
LLM_PROVIDER=gemini
LLM_API_KEY=your-gemini-api-key-here
LLM_MODEL=gemini-2.0-flash

# Service URLs (for docker-compose)
MANAGER_URL=http://manager:8000
WORKER_URL=http://worker:8001
EVALUATOR_URL=http://evaluator:8002

# Optimizer
OPTIMIZER_FAILURE_THRESHOLD=3
OPTIMIZER_LOOKBACK_MINUTES=30

# Logging
LOG_LEVEL=INFO
